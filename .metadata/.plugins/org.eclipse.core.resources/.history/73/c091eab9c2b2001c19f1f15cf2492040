package com.animals.repositories;

import org.springframework.data.jpa.repository.support.JpaRepositoryFactoryBean;
import org.springframework.data.repository.Repository;

import com.animals.models.UsuarioModel;


public class UsuarioDAO extends JpaRepositoryFactoryBean<Repository<UsuarioModel,String>, UsuarioModel, String>{

	public UsuarioRepository(Class<? extends Repository<UsuarioModel, String>> repositoryInterface) {
		super(repositoryInterface);
		// TODO Auto-generated constructor stub
	}

	

}
